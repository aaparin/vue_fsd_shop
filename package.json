    "@vueuse/core": "^10.2.0",
    "aos": "^2.3.4",
    "axios": "^1.4.0",
    "msw": "^1.2.1",
    "pinia": "^2.0.36",
    "sass": "^1.64.1",
    "uuid": "^9.0.0",
    "vue": "^3.3.4",
    "vue-query": "^1.26.0",
